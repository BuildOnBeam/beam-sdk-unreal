{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.9.0",
    "generators": {
      "unreal-player-client": {
        "generatorName": "cpp-ue4",
        "output": "../Source/PlayerClient",
        "templateDir": "templates",
        "inputSpec": "https://api.beta.onbeam.com/api/player-json",
        "skipValidateSpec": false,
        "type-mappings": "string+uri=FString",
        "additionalProperties": {
          "unrealModuleName": "PlayerClient",
          "modelNamePrefix": "PlayerClient"
        }
      }
    }
  }
}